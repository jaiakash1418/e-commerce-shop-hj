{
  "theme": {
    "primaryColor": "#3498db",
    "secondaryColor": "#2ecc71",
    "fontFamily": "Arial, sans-serif",
    "backgroundColor": "#ffffff"
  },
  "layout": {
    "header": {
      "show": true,
      "height": "60px"
    },
    "footer": {
      "show": true,
      "height": "40px"
    },
    "sidebar": {
      "show": true,
      "width": "250px"
    }
  },
  "customizations": {
    "button": {
      "borderRadius": "5px",
      "padding": "10px 20px"
    },
    "input": {
      "border": "1px solid #ccc",
      "borderRadius": "4px",
      "padding": "10px"
    }
  }
}